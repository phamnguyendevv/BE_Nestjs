{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --detectOpenHandles --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- --d src/infrastructure/databases/postgressql/typeorm.config.ts",
    "migration:generate": "cross-env npm run typeorm:generate",
    "migration:create": "npm run typeorm -- migration:create ./database/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- --d src/infrastructure/databases/postgressql/typeorm.config.ts migration:revert",
    "migration:run:prod": "npx typeorm migration:run -d dist/src/infrastructure/databases/postgressql/typeorm.config.js",
    "prepare": "husky",
    "typeorm:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:generate ./database/migrations/$MIGRATION_NAME -d src/infrastructure/databases/postgressql/typeorm.config.ts"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@codegenie/serverless-express": "^4.16.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.9",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.9",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.6",
    "@types/bcrypt": "^5.0.2",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.0.1",
    "cache-manager-ioredis": "^2.1.0",
    "cache-manager-redis-store": "^3.0.1",
    "cacheable": "^1.10.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "ioredis": "^5.6.1",
    "jwks-rsa": "^3.1.0",
    "keyv": "^5.3.4",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^18.3.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.9",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/aws-lambda": "^8.10.147",
    "@types/cache-manager-ioredis": "^2.0.7",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-google-oauth2": "^0.1.10",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/stripe": "^8.0.416",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.0",
    "rimraf": "^6.0.1",
    "serverless-offline": "^13.9.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@domain/(.*)$": "<rootDir>/src/domain/$1",
      "^@use-cases/(.*)$": "<rootDir>/src/use-cases/$1",
      "^@adapters/(.*)$": "<rootDir>/src/adapters/$1",
      "^@infrastructure/(.*)$": "<rootDir>/src/infrastructure/$1",
      "^@modules/(.*)$": "<rootDir>/src/modules/$1",
      "^test/(.*)$": "<rootDir>/test/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": "(\\/test\\/.*\\.spec\\.[jt]s?$)",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/(main|serverless).ts",
      "!src/**/*.module.ts",
      "!src/**/*.config.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
